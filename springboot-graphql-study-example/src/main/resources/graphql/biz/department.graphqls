extend type Query {
    employee(id: ID!): Employee!
}

type Employee {
    id: ID!
    firstName: String!
    lastName: String!
    position: String!
    salary: Int
    age: Int
    department : Department
}

type Department {
    id: ID!
    name: String!
    organization: Organization
}

type Organization {
    id: ID!
    name: String!
}

