extend type Query {
    "根据编号获取员工"
    employee(id: ID!): Employee!
}

"员工"
type Employee {
    id: ID!
    firstName: String!
    lastName: String!
    position: String!
    salary: Int
    age: Int
    department : Department
}

"部分"
type Department {
    id: ID!
    name: String!
    organization: Organization
}

"所属组织"
type Organization {
    id: ID!
    name: String!
}

